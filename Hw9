\newcommand{\newset}[2][]{\ifx\relax#1\relax
     \expandafter\gdef\csname #2\endcsname{\ensuremath{\mathbb{#2}}}
     \expandafter\gdef\csname #2*\endcsname{\ensuremath{\mathfrak{#2}}}
\else
     \expandafter\gdef\csname #2\endcsname{#1}
\fi}


